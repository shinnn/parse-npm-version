{
  "name": "parse-npm-version",
  "version": "0.1.0",
  "description": "Get the currently installed CLI npm version and parse it with node-semver",
  "repository": "shinnn/parse-npm-version",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint --fix --format=codeframe index.js test.js",
    "test": "nyc node test.js"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "npm",
