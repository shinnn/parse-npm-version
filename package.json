    "pinkie-promise": "^2.0.1"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^3.0.0",
    "eslint": "^3.19.0",
    "nyc": "^11.0.2",
    "tape": "^4.6.3"
  },
  "eslintConfig": {
    "extends": "@shinnn/node",
    "rules": {
      "no-var": "off",
      "prefer-arrow-callback": "off"
    }
  }
}
